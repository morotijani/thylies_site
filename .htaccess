RewriteEngine on

RewriteRule ^auth/verify-email/([0-9a-zA-Z]+)$ auth/verify-email.php?vericode=$1 [NC,L]
RewriteRule ^auth/send/([0-9a-zA-Z]+)$ auth/send.php?reseted-password=$1 [NC,L]

RewriteRule ^pmiqn/users/([0-9a-zA-Z]+)$ pmiqn/users.php?view=$1 [NC,L]


#RewriteRule ^verified/([0-9a-zA-Z]+)$ verified.php?vericode=$1 [NC,L]

RewriteCond %{REQUEST_FILENAME} !-d

RewriteCond %{REQUEST_FILENAME}\.php -f

RewriteRule ^(.*)$ $1.php

RewriteCond $1 !^(media|helpers|account|auth|admin|fonts|assests|dist|controller|connection|robots\.txt)

RewriteRule ^([^\.]+)$ $1.php [NC,L]

#ErrorDocument 404 https://sites.local/thylies_site/404.html